    <script>
        numberof = Math.abs(prompt("Enter the number you want the array till"));
        console.log(numberof);
        oprarr = [];
        for(let i=0;i<numberof;i++)
        {
            oprarr[i] = i+1; 
        }
        console.log(oprarr);
        let filoddnumb = oprarr.filter(el=>el%2!=0);
        console.log(filoddnumb);
         let mapcubes = filoddnumb.map(el=>Math.pow(el,3))
         console.log(mapcubes);
    </script>
